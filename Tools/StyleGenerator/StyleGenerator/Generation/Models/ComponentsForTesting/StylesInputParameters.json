{
  "colors": [{
             "name": "lightWhite",
             "color": "F5F5F5"
             }, {
             "name": "lightGrey",
             "color": "F0F0F0"
             }, {
             "name": "grayYellow",
             "color": "D7BE7D"
             }, {
             "name": "grayBlue",
             "color": "C8CDDC"
             }, {
             "name": "grayPurple",
             "color": "AFA5AA"
             }, {
             "name": "grayBlack",
             "color": "373C3C"
             }, {
             "name": "darkBlue",
             "color": "4B5F7D"
             }, {
             "name": "green",
             "color": "7D9682"
             }, {
             "name": "yellow",
             "color": "EBBE14"
             }, {
             "name": "pink",
             "color": "CDA5B4"
             }],
  "fonts": [{
            "fontName": "SFUIText-Regular"
            }, {
            "name": "light",
            "fontName": "SFUIText-Light"
            }, {
            "fontName": "SFUIDisplay-Thin"
            }, {
            "name": "bold",
            "fontName": "ProximaNova-Extrabld"
            }],
  "styles": {
    "text.default": {
      "font.size": 20,
      "font.name": "SFUIText-Light",
      "textColor": "red"
    },
    "text.title": {
      "parents": ["text.default"],
      "font.name": "bold",
      "textColor": "green"
    },
    "button.default": {
      "label": "text.default",
      "cornerRadius": 10,
      "borderColor": "$boarderColor"
    },
    "button.action": {
      "parents": ["button.default", "text.title"],
      "textColor": "$actionColor"
    },
    "button.action.accept": {
      "parents": ["button.default", "text.title"],
      "textColor": "$actionColor"
    },
    "button.action.cancel": {
      "parents": ["button.default", "text.title"],
      "textColor": "$actionColor"
    },
    "textField": {
      "backgroundColor": "yellow"
    }
  },
  "code_templates": {
    "font": "UIFont(.$name, size: $size)",
    "textColor": "UIColor(.$)",
    "backgroundColor": "UIColor(.$)"
  }
}
