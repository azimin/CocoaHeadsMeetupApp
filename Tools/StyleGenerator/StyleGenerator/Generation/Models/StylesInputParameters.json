  "fonts": [{
            "fontName": "SFUIText-Regular"
            }, {
            "name": "light",
            "fontName": "SFUIText-Light"
            }, {
            "fontName": "SFUIDisplay-Thin"
            }, {
            "name": "bold",
            "fontName": "ProximaNova-Extrabld"
            }],

  "styles": [{
    "text.default": {
      "fontName": "light",
      "textColor": "darkBlue",
      "uppercased": 0
             }}, {
    "text.title": {
      "parents": ["text.default"],
      "fontName": "bold",
      "uppercased": 1
             }}, {
